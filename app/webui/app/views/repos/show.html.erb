<%
  @title = "Latest build - #{@repo.long_name}"
  @repo_tab = :latest_build
%>
<% content_for :repo_content do %>
  <% if @wait_for_build %>
    <div class="text-center">
      <h2>Build is now starting. Please wait...</h2>
    </div>
  <% else %>
    <%= render :partial => 'initial_setup' %>
  <% end %>
<% end %>
<%= render 'repos/layout' %>